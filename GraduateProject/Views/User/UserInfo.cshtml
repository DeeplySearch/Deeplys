@model IEnumerable<GraduateProject.Models.User>
@{
    ViewBag.Title = "UserInfo";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<script type="application/x-javascript">
    addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
           function hideURLbar(){ window.scrollTo(0,1); } </script>
<script src="~/js/jquery-2.1.4.min.js"></script>
<link href="~/finalContent/style.css" rel="stylesheet" media="all" />
<link href='//fonts.googleapis.com/css?family=Josefin+Sans:400,100,100italic,300,300italic,400italic,600,600italic,700,700italic' rel='stylesheet' type='text/css'>

<div class="main" >
   
    <div class="main-info">
        <div class="main-info-grid3"style="margin-left:150px">
            <div class="profile-agile" >
                <h5>Profile</h5>
                <div class="w3layouts">
                    @foreach (var Item in Model)
                    {
                        <h3>@Item.FirstName  @Item.LastName</h3>
                        <label class="line"></label>

                        <p>Email: @Item.EmailID</p>
                        <p>Birth Date:@Item.DateOfBirth</p>
                    }
                </div>
                <div class="agileits-w3layouts">

                </div>
            </div>
        </div>
    </div>
    </div>

